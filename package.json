  "scripts": {
    "prebuild": "rimraf dist",
    "build": "swc src -d dist && cp -r src/i18n dist/",
    "start:prod": "node dist/src/main.js",
    "start": "nest start",
    "dev": "lsof -ti:3001 | xargs -r kill -9 && nest start --watch",
    "lint:fix": "eslint --fix \"src/**/*.ts\"",
    "validate-translations": "node scripts/validate-translations.js",
    "start:debug": "nest start --debug --watch",
    "prisma:generate": "prisma generate",
    "prisma:seed": "ts-node --project tsconfig.json prisma/seed/index.ts",
    "postinstall": "pnpm prisma:generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:push": "prisma db push",
    "prisma:studio": "prisma studio",
    "seed": "prisma db seed",
    "test": "jest --config jest.config.js --testRegex '^(?!.*e2e-spec).*\\.spec\\.ts$'",
    "test:watch": "jest --watch --config jest.config.js --testRegex '^(?!.*e2e-spec).*\\.spec\\.ts$'",
    "test:cov": "jest --coverage --config jest.config.js",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --config jest.config.js",
    "test:e2e": "jest --config ./jest-e2e.json --runInBand"
  },
  "prisma": {
    "seed": "pnpm prisma:seed"
  },
